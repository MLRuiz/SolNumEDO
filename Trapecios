function [y]=trapecios(f,x,y0)
%Trapecios versi칩n epsil칩n
    h=x(2)-x(1); %Tama침o de paso
    y=y0(:); %Inicializa
    n=length(x);
    opcion=optimset('Display','off');
    for i=2:n
        xtemp=x(i);ytemp=y(:,i-1);
        xtomp=x(i-1);
        F=@(z) h*(f(xtemp,z)+f(xtomp,ytemp))/2+ytemp-z;
        y(:,i)=fsolve(F,ytemp,opcion); %Euler hacia atr치s
    end
end
