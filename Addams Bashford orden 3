function y=AB3(ode,x,y0)
%Adams-Bashforth (orden 3)
    h=x(2)-x(1); %Tama√±o de paso
    y=y0(:); 
    [n]=length(x);
    ytemp=GL4(ode,x(1:3),y0);%Inicializa
    y(:,2:3)=ytemp(:,2:3); 
    for i=4:n
        y(:,i)=y(:,i-1)+h*(23*ode(x(i-1),y(:,i-1))/12-4*ode(x(i-2),y(:,i-2))/3+...
        5*ode(x(i-3),y(:,i-3))/12);
    end
end
